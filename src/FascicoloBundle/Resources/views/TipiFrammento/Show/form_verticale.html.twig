<div class="clearfix form-horizontal">
	{% for campo in istanza_frammento.frammento.campi %}
		{% if is_visible(campo, alias_fascicolo, path_pagina, istanza_fascicolo) == 'true' %}
			<div class="form-group clearfix">
				<label for="dati_generali_data_marca_da_bollo" class="col-sm-3 col-md-3 col-lg-4 control-label">{{ campo.label }}</label>
				<div class="col-sm-9 col-md-6 col-lg-5 valore-raw">
					{{ istanza_frammento.valoreRawByAlias(campo.alias)|raw }}
				</div>
			</div>
		{% endif %}
	{% endfor %}
</div>