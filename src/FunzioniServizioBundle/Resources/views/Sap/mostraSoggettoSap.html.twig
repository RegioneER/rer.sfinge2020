{% extends '::base.html.twig' %}

{% block body %}
    <p class="lead">SAP test suite - Visualizza beneficiario</p>

    <a href="{{ path('sap_test_suite') }}" type="button">Torna alla suite</a>
    {% if result is not null %}
        <br />
        <br />
        <pre>{{ result|json_encode(constant('JSON_PRETTY_PRINT') b-or constant('JSON_UNESCAPED_UNICODE')) }}
        </pre>

        <a href="{{ path('sap_test_suite') }}" type="button">Torna alla suite</a>
    {% endif %}
{% endblock %}